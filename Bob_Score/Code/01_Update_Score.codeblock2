If actor_var3 == 5
    Set digit=score/10000%10
    ActorAnimation score_digits Frame=digit
    
ElseIf actor_var3 == 4
    Set digit=score/1000%10
    ActorAnimation score_digits Frame=digit
    
ElseIf actor_var3 == 3
    Set digit=score/100%10
    ActorAnimation score_digits Frame=digit
    
ElseIf actor_var3 == 2
    Set digit=score/10%10
    ActorAnimation score_digits Frame=digit
    
ElseIf actor_var3 == 1
    Set digit=score%10
    ActorAnimation score_digits Frame=digit
End